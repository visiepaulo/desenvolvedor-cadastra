{
    "compilerOptions": {
        "outDir": "./dist/",
        "noImplicitAny": true,
        "module": "es6",
        "target": "es5",
        "jsx": "react",
        "allowJs": true,
        "moduleResolution": "node",
        "baseUrl": "./src", // Defina isso como o diretório base para importações
        "paths": {
            "*": ["*"] // Isso permite resolver importações relativas
        }
    }
}
